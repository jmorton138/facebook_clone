<%= @user.email %>
<%= image_tag(@user.avatar, style:'width:20%') if @user.avatar.attached? %>
<%= link_to "Edit profile", edit_user_path(@user.id) %>

<% @posts.each do |post| %>
    <%= post.body %>
<% end %>

<%= link_to "Cancel my account", registration_path(current_user), data: { confirm: "Are you sure?" }, method: :delete %>
